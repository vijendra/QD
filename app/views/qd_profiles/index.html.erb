
<% content_for :header do -%>
   Listing data <%= "[Order details, #{@header_text}]" %>
<% end -%>

<div id='trigger_qd_profile-list' >

<table cellspacing=0 cellpadding=0 id='top-buttons'>  
    <tr><td style ='vertical-align: middle;'> <span style ="color: #04376E;"><b> Act on marked data: &nbsp; </b></span> </td>
       <td style ='vertical-align: top;'> <%= link_to("<span>Print with background </span>", print_file_qd_profiles_url(:format => "pdf", :s => 'true'), :action => :get, :class=> "button")%> </td>
       <td style ='vertical-align: top;'> <%= link_to("<span>Print without background</span>", print_file_qd_profiles_url(:format => "pdf"), :action => :get, :class=> "button")%> </td>
       <td style ='vertical-align: top;'> <%= link_to("<span>Print labels</span>", print_labels_qd_profiles_url(:format => "pdf"), :action => :get, :class=> "button")%> </td>
        <td style ='vertical-align: top;'> <%= link_to("<span>Get csv print file</span>", csv_print_file_qd_profiles_url(:format => "csv"), :action => :get, :class=> "button")%> </td> 
    </tr>
    <tr>    
        <td COLSPAN = "5"><%= 'There are data records pending append. Please refresh after some time to know the status, before generating print file.' unless @pending_append  %> </td>
    </tr>
</table>
<br />  

 <div id = 'top-links' >
   <span id = 's'>
     <%= image_tag('zoom.png') %><%= link_to_function 'Show search bar', "Element.show('search-bar'); Element.show('h'); Element.hide('s'); return false;" %> 
   </span>

   <span id = 'h' style ="display:none">
      <%= link_to_function "Hide search bar", "Element.hide('search-bar'); Element.show('s'); Element.hide('h');return false;" %> 
   </span>
  
    
 </div>
 <div class='clear'> </div>
   <div id = 'search-bar' style ="display:none"> <%=render :partial=> 'filters'%> </div>
<br />
  <div id='qd_profile-list'>
     <%=render :partial=> 'trigger_list'%>
  </div>
</div>
