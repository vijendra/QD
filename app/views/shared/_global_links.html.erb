<% if logged_in? %>

	<% if super_admin? -%>
           <%= link_to "Settings", admin_user_url(current_user) %> &nbsp; | &nbsp; 
        <% else %>
           <% if admin? -%><%= link_to "Settings", edit_admin_administrator_url(current_user) %> &nbsp; | &nbsp;<% end -%>
        <% end -%>

	<% if admin? -%><%= link_to "Administration", admin_root_url %> &nbsp; | &nbsp; <% end -%>
	<%= link_to "Log out", logout_url, :method => :delete %>
<% else %>
	<%= link_to "Sign up", signup_url %> &nbsp;  |  
	<%= link_to "Log in", login_url %>
<% end %>
