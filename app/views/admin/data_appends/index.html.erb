<% content_for :header do %>
   Listing all data sent for append
<% end -%>
 
<table class='listing'>
  <tr>
    <th> File name </th>
    <th> Sent on </th>
    <th> Dealer </th>
    <th> Total records </th>
    <th> Requested by </th>
  </tr>

<% @data_appends.each do |data_append| %>
  <tr class="<%= cycle('odd', 'even') %> no_border">
    <td> <%= data_append.csv_file_name %></td> 
    <td> <%= data_append.created_at.strftime("%d/%m/%Y") %></td> 
    <td> <%= data_append.dealer.profile.name %></td> 
    <td> <%= data_append.no_of_records %></td> 
    <td> <%= data_append.requestor.login %></td> 
  </tr>
  <tr class = "<%= cycle('odd', 'even') %> details"><td  COLSPAN = '5'> <%= data_append.status_message %></td></tr>
<% end %>
</table>

<br />
 
