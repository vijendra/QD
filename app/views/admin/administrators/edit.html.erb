   Editing Administration
<% end -%>



<% form_for([:admin, @administrator]) do |f| %>
  <%= f.error_messages %>
   <p><label for="login">Login</label><br/>
     <%= f.text_field :login %>
  </p>

  <p><label for="email">Contact Email</label><br/>
     <%= f.text_field :email %>
  </p>


  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', admin_administrator_path(@administrator) %> |
