<% form_tag("/admin/dealers/assign_administrator/#{@dealer.id}" ,:method =>:get ) do  %>
<br/>
   <b> <label> Dealer name: <%= @dealer.profile.name %> </label> </b><br/><br/>

<table class ="form-table" >
  <tr> <th> Select an administrator: </th>
   <td>
      <%= select :administrator, :id, [['Select an administrator', '']]+ Administrator.administrators_list,:selected => @dealer.administrator_id %>
   </td>
  </tr>
</table>

<input id="page" name="page" type="hidden" value="<%=h@page%>" />
<input id="per_page" name="per_page" type="hidden" value="<%=h @per_page%>" />
<br/>
<br/>
<%= submit_tag "Assign" %>

<%end%>
