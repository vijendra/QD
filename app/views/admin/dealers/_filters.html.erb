<% form_remote_tag(:url => admin_dealers_url, :method => :get ) do %><%= indicator %>
   <table class="search-table">
     <tr>
       <th> Dealer Name: </th>
       <th> Contact No: </th>
       <th> Email: </th>
       <th> State </th>
       <th> <input type="reset" value="Reset" />  </th>
     </tr>
     <tr>
      <%fields_for @search do |f|%>
        <% f.fields_for @search.conditions do |p| %>
         <% p.fields_for p.object.profile do |profile| %>
               <td><%= profile.text_field :name_like, :class => 'textbox' %></td>
               <td><%= profile.text_field :phone_1_like, :class => 'textbox-tiny' %> -
                   <%= profile.text_field :phone_2_like, :class => 'textbox-tiny' %> -
                   <%= profile.text_field :phone_3_like, :class => 'textbox-tiny' %>
                </td>
         <%end%>
         <td><%= p.text_field :email_like, :class => 'textbox' %></td>
         <td> <%= p.select :state_is ,[['Select' , ''], ['Active', 'active'], ['Inactive', 'inactive']],:class => 'textbox' %> </td>


       <%end%>
       <%end%>
       <td> <%= submit_tag 'Search' %>  </td>
       </tr>
  </table>
<% end %>
